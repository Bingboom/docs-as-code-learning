{
  "command": "AT+CWJAP",
  "title": "连接WiFi网络",
  "type": [
    "设置",
    "查询"
  ],
  "formats": [
    "AT+CWJAP=<ssid>,<pwd>[,<bssid>][,<prio>]",
    "AT+CWJAP?"
  ],
  "parameters": [
    {
      "name": "<ssid>",
      "desc": "WiFi名称",
      "values": {
        "格式": "字符串",
        "长度": "1-32字节"
      }
    },
    {
      "name": "<pwd>",
      "desc": "WiFi密码",
      "values": {
        "格式": "ASCII字符",
        "长度": "8-64字节"
      }
    }
  ],
  "examples": [
    {
      "cmd": "AT+CWJAP=\"MyWiFi\",\"123456\"",
      "resp": "OK"
    },
    {
      "cmd": "AT+CWJAP?",
      "resp": "+CWJAP:\"MyWiFi\",\"aa:bb:cc:dd:ee:ff\",-50,1 OK"
    }
  ],
  "description": "连接到指定WiFi网络",
  "notes": "密码需为8-64字节ASCII字符"
}